<?xml version="1.0"?> 
<robot name="tree">
 <material name="brown">
  <color rgba="0.65 0.39 0.15 0.65"/>
 </material>
    <!-- Base Frame of tree --> 
    <link name="base_link">
        <visual> 
            <geometry> 
                <cylinder length="4" radius="0.25"/>
            </geometry>
           <origin rpy="0 0 0" xyz="0 0 0"/>
           <material name="brown"/>
        </visual>
        <collision>
         <geometry>
         <!-- for tree to stand on the plane -->
         <box size="2.0 2.0 0.5"/>  
         </geometry>
        </collision>
        <inertial>
         <mass value="100"/>
          <inertia ixx="800.0" ixy="0.0" ixz="0.0" iyy="800" iyz="0.0" izz="800"/>
        </inertial>
    </link>

