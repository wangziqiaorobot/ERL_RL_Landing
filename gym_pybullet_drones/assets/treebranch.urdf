    <joint name="base_to_virtual_link" type="revolute">
        <axis xyz="0 0 1"/>
        <origin rpy="0 0 0" xyz="0 0 1.75"/>
        <material name="brown"/>
        <parent link="base_link"/>
        <child link="virtual_link"/>
        <dynamics damping="0.01" friction="0.01"/>
        <limit effort="1000" velocity="0.5" lower="-0.3" upper="0.3" />
    </joint>


    <link name="virtual_link">
        <visual> 
            <geometry> 
                <cylinder length="0.0" radius="0.0"/>
            </geometry>
           <origin rpy="0 0 0" xyz="0 0.0 0"/>
